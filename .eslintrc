{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "env": {
    "node": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "quotes": ["error", "backtick"],
    "no-var": "error",
    "no-console": "warn"
  }
}
